{
  "name": "botforce-unity",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @botforce/web dev",
    "build": "pnpm --filter @botforce/web build",
    "start": "pnpm --filter @botforce/web start",
    "lint": "pnpm --filter @botforce/web lint",
    "typecheck": "pnpm --filter @botforce/web typecheck",
    "test": "pnpm --filter @botforce/web test",
    "test:e2e": "pnpm --filter @botforce/web test:e2e",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
